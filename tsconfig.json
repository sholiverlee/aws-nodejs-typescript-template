{   "compilerOptions": {
        "lib": [
            "ESNext"
        ],
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "ES2020",
        "rootDir": "./",
        "esModuleInterop": true
    },
    "include": [
        "./**/*.ts",
        "serverless.ts"
    ],
    "exclude": [
        "node_modules/**/*",
        ".serverless/**/*",
        ".webpack/**/*",
        "_warmup/**/*",
        ".vscode/**/*"
    ]
}
